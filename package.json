{
  "name": "ipec-coach-connect",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && npm run analyze",
    "build:profile": "tsc && vite build --mode production --profile",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "bundle:size": "npm run build:analyze && echo 'Bundle analysis complete. Check dist/stats.html'",
    "bundle:report": "npm run build && node scripts/bundle-report.js",
    "bundle:analyze": "npm run build && node scripts/bundle-analysis.js",
    "bundle:monitor": "npm run build && node scripts/bundle-analysis.js --monitor",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/**/*.test.{ts,tsx}",
    "test:unit:watch": "vitest src/**/*.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:auth": "playwright test tests/e2e/auth.spec.ts",
    "test:e2e:booking": "playwright test tests/e2e/coach-booking.spec.ts",
    "test:accessibility": "playwright test tests/e2e/accessibility.spec.ts",
    "test:performance": "playwright test tests/e2e/performance.spec.ts",
    "test:visual": "playwright test --grep='visual'",
    "test:mobile": "playwright test --grep='mobile'",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "preview": "vite preview",
    "build:staging": "NODE_ENV=staging npm run typecheck && vite build --mode staging",
    "build:production": "NODE_ENV=production npm run typecheck && vite build --mode production",
    "build:preview": "npm run build:production && vite preview",
    "health-check": "node scripts/health-check.js",
    "deploy:preview": "vercel",
    "deploy:staging": "npm run build:staging && vercel --target staging",
    "deploy:production": "npm run build:production && vercel --prod",
    "postbuild": "echo 'Build completed successfully. Checking output...' && ls -la dist/ && du -sh dist/",
    "verify:production": "node scripts/verify-production-readiness.js",
    "predeploy:production": "npm run verify:production",
    "security:validate": "node scripts/validate-security-config.cjs",
    "security:check": "npm audit --audit-level=moderate && npm run security:validate"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.16",
    "@stripe/react-stripe-js": "^4.0.0",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.57.2",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^3.3.1",
    "embla-carousel-react": "^8.0.0",
    "framer-motion": "^11.0.8",
    "geolib": "^3.3.4",
    "immer": "^10.1.1",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.542.0",
    "otpauth": "^9.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.8.1",
    "react-router-dom": "^6.22.3",
    "stripe": "^14.19.0",
    "web-vitals": "^3.5.0",
    "zod": "^4.1.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/plugin-transform-react-jsx-development": "^7.22.5",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "cssnano": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^7.1.4",
    "vite-bundle-analyzer": "^1.1.0",
    "vite-plugin-compression2": "^2.2.1",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  }
}
